    "react-container-dimensions": "^1.4.1",
    "react-countdown": "^2.3.1",
    "react-lazy-load": "^3.0.13",
    "react-lottie": "^1.2.3",
    "react-masonry-component": "^6.2.1",
    "react-select": "^2.2.0",
